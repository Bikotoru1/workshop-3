{
  "name": "workshop-3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "test": "jest",
    "format": "prettier --write \"src/**/*.{js,vue}\"",
    "format:css": "prettier --write \"src/**/*.{css,scss}\"",
    "format:html": "prettier --write \"src/**/*.{html}\"",
    "format:json": "prettier --write \"src/**/*.{json}\"",
    "format:md": "prettier --write \"src/**/*.{md}\"",
    "format:check": "prettier --check \"src/**/*.{js,vue}\"",
    "format:check:css": "prettier --check \"src/**/*.{css,scss}\"",
    "format:check:html": "prettier --check \"src/**/*.{html}\"",
    "format:check:json": "prettier --check \"src/**/*.{json}\"",
    "format:check:md": "prettier --check \"README.md\""
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.8.3",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/eslint-parser": "^7.12.16",
    "@babel/preset-env": "^7.24.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/cli-service": "~5.0.0",
    "@vue/test-utils": "^1.2.1",
    "@vue/vue2-jest": "27.0.0",
    "babel-jest": "^27.5.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.5.1",
    "prettier": "^2.4.1",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}
